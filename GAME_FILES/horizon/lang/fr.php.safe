$translation['NOUVEAUTE_781']= "<!-- Indique les informations sur les deux joueurs et leurs planètes -->\n    <div style=\"text-align : justify; margin-bottom : 20px;\">\n        La flotte du joueur %s%s \n        vient d’arriver sur l’orbite de la planète %s [%s:%s] du joueur \n        %s%s\n        pour attaquer le peuple de celle-ci. Cette flotte provient de la planète %s [%s:%s].<br><br>\n        \n        Vous pouvez dès à présent lire ce rapport de combat qui s’est déroulé le %s.\n	</div>\n    \n    <!-- Début du rapport de combat -->\n    <div style=\"width : 620px; text-align : center; margin : 20px auto;\">\n        \n                    \n            \n            \n                            <!-- Affiche le titre de cette passe -->\n                                                   <h3>Les flottes avant le combat :</h3>\n                \n                \n                \n                                    \n                                                            \n                    <div class=\"partie_rapport\">\n                        <!-- Titre du tableau en fonction du joueur affiché -->\n                        <div class=\"titre_tableau\">\n                            Flotte de l’attaquant « <span class=\"rouge\">%s</span> »\n                        </div>\n\n                        \n                                                    <table class=\"combat\">\n                                <!-- Les en-têtes du tableau -->\n                                <tbody><tr>\n                                    <th>Nom</th>\n                                    <th>Attaque</th>\n                                    <th>Bouclier</th>\n                                    <th>Coque</th>\n                                    <th>Nombre</th>\n                                </tr>\n                                \n                                \n                                                                    \n                                                                                                                                                                    \n                                            <!-- Affiche les caractéristiques du modèle -->\n                                            %s\n                                    \n                                \n                                                            </tbody></table>\n \n                                            </div>\n                                    \n                                                            \n                    <div class=\"partie_rapport\">\n                        <!-- Titre du tableau en fonction du joueur affiché -->\n                        <div class=\"titre_tableau\">\n                            Flotte du défenseur « <span class=\"cyan\">%s</span> »\n                        </div>\n\n                        \n                                                    <table class=\"combat\">\n                                <!-- Les en-têtes du tableau -->\n                                <tbody><tr>\n                                    <th>Nom</th>\n                                    <th>Attaque</th>\n                                    <th>Bouclier</th>\n                                    <th>Coque</th>\n                                    <th>Nombre</th>\n                                </tr>\n                                \n                                \n                                                                    \n                                                                                                                                                                    \n                                            <!-- Affiche les caractéristiques du modèle -->\n                                            %s\n                                          \n                                                            </tbody></table>\n                            \n\n                                            </div>\n                                            <!-- Affiche le titre de cette passe -->\n                                   <h3>Les flottes après le combat (passe n°%s) :</h3>\n                \n                \n                \n                                    \n                                                            \n                    <div class=\"partie_rapport\">\n                        <!-- Titre du tableau en fonction du joueur affiché -->\n                        <div class=\"titre_tableau\">\n                            Flotte de l’attaquant « <span class=\"rouge\">%s</span> »\n                        </div>\n\n                        \n                                                    <table class=\"combat\">\n                                <!-- Les en-têtes du tableau -->\n                                <tbody><tr>\n                                    <th>Nom</th>\n                                    <th>Attaque</th>\n                                    <th>Bouclier</th>\n                                    <th>Coque</th>\n                                    <th>Nombre</th>\n                                </tr>\n                                \n                                \n                                                                    <!-- Le joueur n’a plus de vaisseau -->\n                                   %s\n                                                            </tbody></table>\n                            \n                        \n                        \n                        \n                                            </div>\n                                    \n                                                            \n                    <div class=\"partie_rapport\">\n                        <!-- Titre du tableau en fonction du joueur affiché -->\n                        <div class=\"titre_tableau\">\n                            Flotte du défenseur « <span class=\"cyan\">%s</span> »\n                        </div>\n\n                        \n                                                    <table class=\"combat\">\n                                <!-- Les en-têtes du tableau -->\n                                <tbody><tr>\n                                    <th>Nom</th>\n                                    <th>Attaque</th>\n                                    <th>Bouclier</th>\n                                    <th>Coque</th>\n                                    <th>Nombre</th>\n                                </tr>\n                                \n                                \n                                                                    \n                                                                                                                                                                    \n                                            <!-- Affiche les caractéristiques du modèle -->\n                                           %s\n                                                                                                                \n                                  \n                                \n                                                            </tbody></table>\n                            \n                        \n                        \n                        \n                                            </div> \n                                            \n        \n        <!-- Affichage de la conclusion du rapport -->\n        <h3>Conclusion du rapport de combat :</h3>\n        <div style=\"text-align : justify; padding-top : 5px;\">\n            \n                           %s\n                \n            \n            \n\n            \n                            <br>\n                %s\n            \n\n            \n                    </div>\n    </div>\n</div>";
